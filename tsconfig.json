{
  "compilerOptions": {
    "baseUrl": "src",
    "esModuleInterop": true,
    "module": "commonjs",
    "outDir": "dist",
    "paths": {
      "@commands/*": ["commands/*"],
      "@configs/*": ["configs/*"],
      "@constants/*": ["constants/*"],
      "@events/*": ["events/*"],
      "@handlers/*": ["handlers/*"],
      "@interfaces/*": ["interfaces/*"],
      "@Metabot": ["Metabot.js"],
      "@services/*": ["services/*"],
      "@utils/*": ["utils/*"]
    },
    "resolveJsonModule": true,
    "rootDir": "src",
    "sourceMap": false,
    "target": "es2020",
    "types": ["node"]
  }
}
